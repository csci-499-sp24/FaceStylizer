    "@nextui-org/system": ">=2.0.0"
  },
  "dependencies": {
    "@react-aria/focus": "^3.14.3",
    "@react-aria/utils": "^3.21.1",
    "@nextui-org/button": "2.0.27",
    "@nextui-org/shared-utils": "2.0.4",
    "@nextui-org/react-utils": "2.0.10",
    "@nextui-org/shared-icons": "2.0.6",
